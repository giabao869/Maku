-- ===== LOAD UI =====
local Redz = loadstring(game:HttpGet(
    "https://raw.githubusercontent.com/daucobonhi/Ui-Redz-V2/refs/heads/main/UiREDzV2.lua"
))()

-- ===== CONFIG =====
local Players = game:GetService("Players")
local LocalPlayer = Players.LocalPlayer
local uid = LocalPlayer.UserId

local KEY_LIFE = 24 * 60 * 60 -- ⬅️ SỬA THỜI GIAN Ở ĐÂY (giây)
local SAVE_FILE = "LuckyGaming_KeyTime.txt"

local randomKey = "Lucky-" .. uid .. "-" .. os.date("%d%m")

if setclipboard then
    setclipboard(randomKey)
end

-- ===== LOAD TIME =====
local function getExpire()
    if isfile and readfile and isfile(SAVE_FILE) then
        local t = tonumber(readfile(SAVE_FILE))
        if t and t > os.time() then
            return t
        end
    end
    return nil
end

local function saveExpire()
    if writefile then
        writefile(SAVE_FILE, tostring(os.time() + KEY_LIFE))
    end
end

local expireTime = getExpire()

-- ===== UI =====
MakeWindow({
    Hub = {
        Title = expireTime
            and ("Lucky Gaming | Còn " .. math.floor((expireTime - os.time()) / 3600) .. "h")
            or "Lucky Gaming | Chưa kích hoạt",
        Animation = "By: Lucky Gaming"
    },
    Key = {
        KeySystem = expireTime == nil,
        Title = "Key System",
        Description = "Key của bạn: " .. randomKey,
        Keys = {randomKey},
        Notifi = {
            Notifications = true,
            CorrectKey = "Key đúng ✅",
            Incorrectkey = "Sai key ❌",
            CopyKeyLink = randomKey
        }
    }
})

-- ===== SAVE CHỈ 1 LẦN SAU KHI NHẬP KEY =====
if expireTime == nil and not (isfile and isfile(SAVE_FILE)) then
    saveExpire()
    expireTime = getExpire()
end

-- ===== UPDATE TITLE REALTIME =====
task.spawn(function()
    while task.wait(1) do
        if expireTime then
            local remain = expireTime - os.time()
            if remain <= 0 then
                if delfile then pcall(delfile, SAVE_FILE) end
                break
            end
            pcall(function()
                Redz:SetTitle(
                    "Lucky Gaming | Còn " ..
                    math.floor(remain / 3600) .. "h " ..
                    math.floor((remain % 3600) / 60) .. "p"
                )
            end)
        end
    end
end)

local MiniButton = MinimizeButton({
    Image = "rbxassetid://136573518692872",
    Size = {40, 40},
    Color = Color3.fromRGB(0, 255, 255),
    Corner = true,
    Stroke = true,
    StrokeColor = Color3.fromRGB(0, 255, 0)
})

task.spawn(function()
    while task.wait(0.1) do
        local color = Color3.fromHSV(tick() % 5.5 / 5.5, 1, 1)
        pcall(function()
            -- Quét sạch CoreGui để tìm Menu và Nút
            for _, gui in pairs(game:GetService("CoreGui"):GetChildren()) do
                if gui:IsA("ScreenGui") then
                    
                    -- 1. Tìm và nhuộm màu Nút Nhỏ (Minimize)
                    for _, obj in pairs(gui:GetDescendants()) do
                        if obj:IsA("ImageButton") and obj.Image == "rbxassetid://136573518692872" then
                            local s = obj:FindFirstChildOfClass("UIStroke")
                            if s then s.Color = color end
                        end
                        
                        -- 2. Tìm tất cả viền (UIStroke) trong bất kỳ Menu nào đang mở
                        if obj:IsA("UIStroke") then
                            obj.Color = color
                        end
                    end
                    
                    -- 3. Ép màu Accent cho khung chính (Main)
                    local main = gui:FindFirstChild("Main", true)
                    if main then
                        pcall(function() main.AccentColor = color end)
                    end
                end
            end
        end)
    end
end)
      
